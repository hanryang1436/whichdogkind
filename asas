                    case "dog":
                        labelTitle = "강아지상"
                        break;
                    case "cat":
                        labelTitle = "고양이상"
                        break;
                    case "rabbit":
                        labelTitle = "토끼상"
                        break;
                    case "dinosaur":
                        labelTitle = "공룡상"
                        break;
                    case "bear":
                        labelTitle = "곰상"
                        break;
                    case "deer":
                        labelTitle = "사슴상"
                        break;
                    case "fox":
                        labelTitle = "여우상"
                        break;
                    default:
                        labelTitle = "알수없음"

